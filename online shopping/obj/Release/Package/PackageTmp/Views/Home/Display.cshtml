@model List<online_shopping.Models.client>

@{
    ViewBag.Title = "Display";
}

<h2>Display</h2>

<h2> List of Clients</h2>

<table border="1" align="center" cellspacing="12" cellpadding="5">
    <tr>
        <th>
            Id

        </th>
        
        <th>
            name
        </th>
        <th>
            C_email
        </th>
        <th>
            C_password
        </th>

        
    </tr>

    @foreach (var c in Model)
    {
        <tr>
            <td>
                @c.Id

            </td>

            <td>
                @c.name
            </td>
            <td>
                @c.C_email
            </td>
            <td>
                @c.C_password
            </td>
            

          
        </tr>
    }

</table>






